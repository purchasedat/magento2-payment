<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Store/etc/config.xsd">
    <default>
        <payment>
            <purchasedat>
                <active>1</active>
                <title>Purchased.at</title>
                <order_status>processing</order_status>
                <test>1</test>
                <instructions>You will be redirected to Purchased.at</instructions>
                <payment_action>true</payment_action>
                <model>PurchasedAt\Magento2Payment\Model\PurchasedatModel</model>
                <payment_failed_template>payment_us_purchasedat_payment_pat_failed_template</payment_failed_template>
            </purchasedat>
        </payment>
    </default>
</config>
